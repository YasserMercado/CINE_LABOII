              <DbSource ConnectionRef="desktop-47s0gds.CINE_CORDOBA.dbo" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PEL.nombre_pelicula AS PELICULAS, CONVERT(varchar, ENT.fecha_entrada, 3) AS FECHA, CAST(DET.descuento AS varchar) + '%' AS DESCUENTO, COM.monto AS MONTO, COM.cantidad AS CANTIDAD
FROM            DETALLES_COMPROBANTES AS DDC INNER JOIN
                         COMPROBANTES AS COM ON DDC.id_detalle_comprobante = COM.id_detalle_comprobante INNER JOIN
                         ENTRADAS AS ENT ON COM.id_entrada = ENT.id_entrada INNER JOIN
                         DESCUENTOS AS DET ON DDC.id_descuento = DET.id_descuento INNER JOIN
                         FUNCIONES AS FUN ON FUN.id_funcion = ENT.id_entrada INNER JOIN
                         PELICULAS AS PEL ON PEL.id_pelicula = PEL.id_pelicula
WHERE        (DDC.id_forma_pago IN
                             (SELECT        id_forma_pago
                               FROM            FORMAS_PAGOS AS FDP
                               WHERE        (id_forma_pago = DDC.id_forma_pago) AND (forma_pago LIKE '%tarjeta%'))) AND (DATEDIFF(MONTH, ENT.fecha_entrada, GETDATE()) = 3) AND (DET.descuento &gt; 20)
ORDER BY DESCUENTO</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
            <Mappings>
              <Mapping SourceColumn="PELICULAS" DataSetColumn="PELICULAS" />
