                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SUM(c.monto) AS Total, 'Efectivo' AS Metodo
FROM            COMPROBANTES AS c INNER JOIN
                         DETALLES_COMPROBANTES AS d ON c.id_detalle_comprobante = d.id_detalle_comprobante INNER JOIN
                         FORMAS_PAGOS AS fp ON d.id_forma_pago = fp.id_forma_pago INNER JOIN
                         FORMAS_COMPRAS AS fc ON c.id_forma_compra = fc.id_forma_compra INNER JOIN
                         ENTRADAS AS e ON c.id_entrada = e.id_entrada
