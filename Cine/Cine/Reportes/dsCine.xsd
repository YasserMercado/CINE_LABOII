            <MainSource>
              <DbSource ConnectionRef="desktop-47s0gds.CINE_CORDOBA.dbo" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        BUT.fila + CAST(BUT.numero AS varchar) AS BUTACA, CLI.nombre_cliente + ' ' + CLI.apellido_cliente AS CLINTE, COUNT(ENT.fecha_entrada) AS ASISTENCIAS
FROM            BUTACAS AS BUT INNER JOIN
                         DETALLES_SALAS AS DDS ON BUT.id_butaca = DDS.id_butaca INNER JOIN
                         ENTRADAS AS ENT ON DDS.id_detalle_sala = ENT.id_detalle_sala INNER JOIN
                         CLIENTES AS CLI ON CLI.id_cliente = ENT.id_cliente INNER JOIN
                         FUNCIONES AS FUN ON FUN.id_funcion = ENT.id_funcion INNER JOIN
                         PELICULAS AS PEL ON PEL.id_pelicula = FUN.id_pelicula
WHERE        EXISTS
                             (SELECT        clasificacion
                               FROM            CLASIFICACIONES AS CLA
                               WHERE        (clasificacion = 'G'))
GROUP BY BUT.fila + CAST(BUT.numero AS varchar), CLI.nombre_cliente + ' ' + CLI.apellido_cliente
HAVING        (COUNT(ENT.fecha_entrada) &gt; 3)
ORDER BY ASISTENCIAS DESC</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
