            <MainSource>
              <DbSource ConnectionRef="desktop-47s0gds.CINE_CORDOBA.dbo" DbObjectName="CINE_CORDOBA.dbo.PELICULAS" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        p.nombre_pelicula AS Pelicula, SUM(c.monto) AS Total, COUNT(ds.id_butaca) AS Butacas
FROM            COMPROBANTES AS c INNER JOIN
                         DETALLES_COMPROBANTES AS dc ON dc.id_detalle_comprobante = c.id_detalle_comprobante INNER JOIN
                         ENTRADAS AS en ON en.id_entrada = c.id_entrada INNER JOIN
                         DETALLES_SALAS AS ds ON ds.id_detalle_sala = en.id_detalle_sala INNER JOIN
                         SALAS AS sa ON sa.id_sala = ds.id_sala INNER JOIN
                         FUNCIONES AS fu ON fu.id_funcion = en.id_funcion INNER JOIN
                         PELICULAS AS p ON p.id_pelicula = fu.id_pelicula INNER JOIN
                         BUTACAS AS b ON b.id_butaca = ds.id_butaca
GROUP BY p.nombre_pelicula
HAVING        (SUM(c.monto) &gt;
                             (SELECT        AVG(cantidad * monto) AS Expr1
                               FROM            COMPROBANTES)) AND (COUNT(ds.id_butaca) &lt; 10)</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
