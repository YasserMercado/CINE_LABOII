          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="dsDescuentoTarjetaTableAdapter" GeneratorDataComponentClassName="dsDescuentoTarjetaTableAdapter" Name="dsDescuentoTarjeta" UserDataComponentName="dsDescuentoTarjetaTableAdapter">
